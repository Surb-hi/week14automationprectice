<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="regression Suits" verbose="1">
    <groups>
        <run>
            <include name="regression"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.automation.customlisteners.CustomListeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="regression">
        <classes>
            <class name="com.automation.testsuits.CreateAccountPageTest"></class>
            <class name="com.automation.testsuits.SignPageTest"></class>
            <class name="com.automation.testsuits.WomenCategoryPageTest"></class>
        </classes>

    </test>
</suite>